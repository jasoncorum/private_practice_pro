<% provide(:title, 'Therapist Profiles') %>
<h2>Find a Therapist</h2>
<p>Find a therapist by zip code.</p>
</div>

<% @profiles.each do |profile| %>

<div class="container">
<%= link_to image_tag(profile.image.url(:thumb)), profile %>
<strong>First Name: <%= profile.first_name %></strong></br>
<strong>Last Name: <%= profile.last_name %></strong></br>

<hr>

</div>
<% end %>